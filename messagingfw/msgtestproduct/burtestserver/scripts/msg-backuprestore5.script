//! @SYMTestType 		CIT

RUN_TEST_STEP 300 T_MsgCommonServer 	StartUp
RUN_TEST_STEP 300 T_MsgImapServer 	StartUp
RUN_TEST_STEP 300 T_MsgSmtpServer	StartUp

RUN_TEST_STEP 300 T_MsgCommonServer 	InitNtRas			c:\msgtest\backup\msg-backuprestore5.ini 	0004-InitNtRas
RUN_TEST_STEP 300 T_MsgCommonServer 	ResetMessageStore		c:\msgtest\backup\msg-backuprestore5.ini 	0004-ResetMessageStore
RUN_TEST_STEP 300 T_MsgImapServer 	CreateImapAccount		c:\msgtest\backup\msg-backuprestore5.ini 	0004-CreateImapAccount

START_REPEAT	c:\msgtest\backup\msg-backuprestore5.ini	0001-SectionOne	RepeatParam
RUN_TEST_STEP 300 T_MsgSmtpServer 	CreateSmtpMessageFromEmailFile	c:\msgtest\backup\msg-backuprestore5.ini 	0004-CreateSmtpMessageFromEmailFile1
END_REPEAT

RUN_TEST_STEP 300 T_MsgSmtpServer	ShutDown
RUN_TEST_STEP 300 T_MsgImapServer 	ShutDown
RUN_TEST_STEP 300 T_MsgCommonServer 	ShutDown

END_TESTCASE MSG-FULL-INCREMENTAL-BACKUPRESTORE-0004

RUN_SCRIPT c:\msgtest\backup\msg-concurrent-bur-move-oprs4.script

